{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "airbnb/hooks"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "rules": {
      "linebreak-style": ["error", "windows"],
      "quotes": ["error", "double"],
      "import/extensions": "off",
      "react/jsx-boolean-value": "off"
  }
}
