{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        // project options
        "lib": [
            "DOM",
        ], // specifies which default set of type definitions to use ("DOM", "ES6", etc)
        "outDir": "../public/js/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        // Module resolution
        "baseUrl": "./", // Lets you set a base directory to resolve non-absolute module names.

    },
    "include": ["./App.tsx"],
}